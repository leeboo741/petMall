/* pages/login/register/index.wxss */
.button-confirm{
  font-size: 34rpx;
  margin: 20rpx 30rpx;
  height: 90rpx;
  line-height: 90rpx;
  background: linear-gradient(to right, rgb(255,147,87) , rgb(255, 51, 59));
  color: white;
}

.get-code-button{
  padding: 10rpx 15rpx;
  border-radius: 8rpx;
}

.non-null-icon{
  color: #ee2c2c;
  font-size: 32rpx;
  font-weight: bold;
}

.get-able{
  background: white;
  border: 1rpx solid #778899;
}

.get-disable{
  background: rgb(236, 236, 236);
  border: 1rpx solid #778899;
}