.search-box{
  margin: 10rpx auto;
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
}

.search-content{
  height: 80rpx;
  border-radius: 30rpx;
  background: #f8f8f9;
  display: flex;
  flex-direction: row;
  align-items: center;
  padding: 0rpx 100rpx;
  color: #bbbec4;
  position: relative;
}

.search-content-mask{
  position: absolute;
  width: 100%;
  height: 100%;
  top: 0rpx;
  left: 0rpx;
  z-index: 2;
}

.search-input{
  width: 350rpx;
}

.banner{
  width: 100%;
  height:250rpx;
}

.banner-item{
  position: absolute;
  top: 0rpx;
  left: 0rpx;
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
}

.banner-item-image{
  width: 100vw;
  height: 250rpx;
  margin: auto;
  border-radius: 10rpx;
  /* box-shadow: 0rpx 0rpx 40rpx 5rpx #b9b9b9; */
}

.fast-action-area{
