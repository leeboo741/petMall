.text-name{
  font-size: 28rpx;
  font-weight: bold;
  color: #1c2438;
  display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
}

.text-price{
  color: #ee2c2c;
  font-size: 22rpx;
  display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
  font-weight: bold;
}

.text-info{
  color: #bbbec4;
  font-size: 22rpx;
}

.text-address{
  color: #bbbec4;
  font-size: 20rpx;
   display:-webkit-box;
  -webkit-line-clamp:1;
  overflow:hidden;
  text-overflow:ellipsis;
  -webkit-box-orient:vertical;
  word-break:break-all;
  max-width: 50%;
}

.text-original-price{
  color: #bbbec4;
  font-size: 22rpx;
  text-decoration: line-through;
}

.text-time {
  color: #bbbec4;
  font-size: 22rpx;
}

